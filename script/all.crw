(
    .default "hello"
    .level 10

    .prop-func '(print default " " name)
    (prop-func .name "sally")

    .list-func '(print default " hello you are ? " value)
    (list-func "yo" "dummy" "nail" "scratch")

    .nested-func (
        if (false)
                '(print "the right answer is " (+ 1))
            (true)
                '(print "the wrong answer is " (+ 3)))
    (nested-func)

    (print .data))
